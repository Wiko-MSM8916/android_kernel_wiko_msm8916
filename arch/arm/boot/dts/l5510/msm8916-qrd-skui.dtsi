	ktd@30 { // ktd2026 field sensor //wangyanhui add 20141120
		compatible = "ktd,ktd2026";
		reg = <0x30>;
	};
};

&sdc2_cd_off {
	/delete-property/ bias-disable;
	bias-pull-down;
};

&sdhc_2 {
	interrupts = <0 1>;
	interrupt-map = <0 &intc 0 125 0
		1 &intc 0 221 0>;
	interrupt-names = "hc_irq", "pwr_irq";
	/delete-property/ cd-gpios;
};

