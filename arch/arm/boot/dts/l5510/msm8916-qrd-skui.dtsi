	hall-ic@7e {
		compatible = "hall-switch";
    	    	reg = <0x7e>;
		pinctrl-names = "default";
		pinctrl-0 = <&hall_sensor_int_default>;
		interrupt-parent = <&msm_gpio>;
		interrupts = <117 0x2003>;
		vddio-supply = <&pm8916_l6>;
		linux,gpio-int = <&msm_gpio 117 0x1>;
		linux,wakeup;
		linux,min-uv = <1800000>;
		linux,max-uv = <1800000>;
	};

	sound { /*tinno wangyanhui modify 0911*/
		compatible = "qcom,msm8x16-audio-codec";
		qcom,model = "msm8x16-skui-snd-card";
		qcom,msm-snd-card-id = <0>;
		qcom,msm-ext-pa = "primary";
		qcom,msm-codec-type = "internal";
		qcom,msm-mbhc-hphl-swh = <1>;
		qcom,msm-mbhc-gnd-swh = <0>;
		qcom,msm-hs-micbias-type = "internal";
		qcom,msm-mclk-freq = <12288000>;
		//2014/11/18 tyj. added msm-micbias1-ext-cap to indicate ext cap is used for l5510.
		qcom,msm-micbias1-ext-cap;
		//qcom,msm-spk-ext-pa = <&msm_gpio 16 0>;
		qcom,ext-spk-amp-gpio = <&msm_gpio 16 0>;//yangliang add for external padac for spk;20150708
