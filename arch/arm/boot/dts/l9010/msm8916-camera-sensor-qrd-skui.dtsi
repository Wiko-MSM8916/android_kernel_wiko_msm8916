       lm3642-en-pin {
            qcom,pins = <&gp 36>, <&gp 86>;
            qcom,num-grp-pins = <2>;
            label = "lm3642_en_pins";
            qcom,pin-func = <0>;

            lm3642_en_default: en-default {
                       driver-strength = <2>;
                       bias-disable;
            };  
            
            lm3642_en_suspend: en-suspend {
                driver-strength = <2>;
                bias-pull-down;
            };  
    }; 
};

